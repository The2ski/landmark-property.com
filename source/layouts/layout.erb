<!doctype html>
<html>
    <head>
        <!-- Meta -->
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="description" content="<%= defined?(description) ? description : current_page.data.description %>">
        <meta name="author" content="Landmark Property Services, Inc.">
        <meta name="keywords" content="<%= defined?(keywords) ? keywords.join(', ') : current_page.data.keywords %>">

        <% if defined?(property) %>
        <meta name="geo.position" content="<%= property['lat'] %>;<%= property['lng'] %>" />
        <meta name="geo.placename" content="<%= property['city'] %>" />
        <meta name="geo.region" content="US-VA" />
        <meta name="ICBM" content="<%= property['lat'] %>, <%= property['lng'] %>" />
        <% end %>

        <!-- Favicon -->
        <link rel="shortcut icon" href="/assets/img/landmark/favicon.png">

        <!-- Web Fonts -->
        <link rel='stylesheet' type='text/css' href='//fonts.googleapis.com/css?family=Open+Sans:400,300,600&amp;subset=cyrillic,latin'>

        <%= stylesheet_link_tag '/assets/plugins/bootstrap/css/bootstrap.min' %>
        <%= stylesheet_link_tag 'style' %>
        <%= stylesheet_link_tag 'headers/header-v6' %>
        <%= stylesheet_link_tag 'footers/footer-v1' %>
        <%= stylesheet_link_tag '/assets/plugins/animate' %>
        <%= stylesheet_link_tag '/assets/plugins/line-icons/line-icons' %>
        <%= stylesheet_link_tag '/assets/plugins/sky-forms-pro/skyforms/css/sky-forms' %>
        <%= stylesheet_link_tag '/assets/plugins/font-awesome/css/font-awesome.min' %>
        <%= stylesheet_link_tag '/assets/plugins/revolution-slider/rs-plugin/css/settings' %>
        <%= stylesheet_link_tag '/assets/plugins/cube-portfolio/cubeportfolio/css/cubeportfolio.min' %>
        <%= stylesheet_link_tag '/assets/plugins/cube-portfolio/cubeportfolio/custom/custom-cubeportfolio' %>
        <%= stylesheet_link_tag 'pages/profile' %>
        <%= stylesheet_link_tag 'pages/shortcode_timeline2' %>
        <%= stylesheet_link_tag 'pages/page_job_inner2' %>
        <%= stylesheet_link_tag 'custom' %>
        <!--[if lt IE 9]><link rel="stylesheet" href="assets/plugins/revolution-slider/rs-plugin/css/settings-ie8.css" type="text/css" media="screen"><![endif]-->

        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyApXnDaoLqJmYYEj-jXMVihlYnjhdNv3xA"></script>
        <script src="/assets/plugins/jquery/jquery.min.js"></script>

        <title><%= defined?(title) ? title : (current_page.data.title || "Landmark Property Services, Inc.") %></title>
    </head>

    <body class="<%= page_classes %> header-fixed header-fixed-space">
        <div class='wrapper'>
            <%= partial 'header' %>
            <%= yield %>
            <%= partial 'footer' %>
        </div>

        <%= javascript_include_tag  "all" %>
        <script>
            (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
            })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

            ga('create', 'UA-67751777-1', 'auto');
            ga('send', 'pageview');
        </script>
    </body>
</html>
